<!-- 新規投稿画面 -->
<h2>New book</h2>
<%= form_for(@book) do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <p><%= f.submit 'Create Book' %></p>
<% end %>
